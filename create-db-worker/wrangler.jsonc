				"name": "CREATE_DB_RATE_LIMITER",
				"type": "ratelimit",
				"namespace_id": "1005",
				"simple": {
					"limit": 100,
					"period": 60,
				},
			},
			{
				"name": "PROGRAMMATIC_RATE_LIMITER",
				"type": "ratelimit",
				"namespace_id": "1006",
				"simple": {
					"limit": 5,
					"period": 60,
				},
			},
		],
	},
