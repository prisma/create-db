{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "create-db-worker",
	"main": "src/index.ts",
	"account_id": "16b32bbb36161aca01a6357a37bc453e",
	"compatibility_date": "2025-06-27",
	"keep_vars": true,
	"observability": {
		"enabled": true,
	},
	"workflows": [
		{
			"name": "delete-db-workflow",
			"binding": "DELETE_DB_WORKFLOW",
			"class_name": "DeleteDbWorkflow",
		},
		{
			"name": "delete-stale-workflow",
			"binding": "DELETE_STALE_WORKFLOW",
			"class_name": "DeleteStaleProjectsWorkflow",
		},
	],
	"triggers": {
		"crons": ["0 0 * * *"],
	},
